{
  "name": "Publication",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "string",
      "required": true
    },
    "Special": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "Image": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "messages": {
      "type": "hasMany",
      "model": "Message",
      "foreignKey": ""
    },
    "salseman": {
      "type": "belongsTo",
      "model": "Salseman",
      "foreignKey": "salsemanId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
